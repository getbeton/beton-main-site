{
  "framework": "astro",
  "headers": [
    {
      "source": "/fonts/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=86400, stale-while-revalidate=604800" }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" }
      ]
    }
  ],
  "redirects": [
    { "source": "/all-enrichment-data-sources-and-destinations", "destination": "/integrations/", "permanent": true },
    { "source": "/docs", "destination": "https://docs.getbeton.ai", "permanent": false },
    { "source": "/blog", "destination": "https://blog.getbeton.ai", "permanent": false },
    { "source": "/app", "destination": "https://inspector.getbeton.ai", "permanent": false }
  ]
}
